<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="222.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bugtracker23.register.RegistrationController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Create a new account">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="54.0" text="Fill out the fields below and click okay to create your account or cancel to exit." />
      <Label layoutX="13.0" layoutY="85.0" text="Username:" />
      <TextField fx:id="usernameField" layoutX="14.0" layoutY="102.0" />
      <Label layoutX="15.0" layoutY="127.0" text="Password:" />
      <PasswordField fx:id="passwordField" layoutX="14.0" layoutY="144.0" />
      <Button fx:id="okButton" layoutX="53.0" layoutY="175.0" mnemonicParsing="false" onAction="#saveUser" prefHeight="25.0" prefWidth="52.0" text="OK" />
      <Button layoutX="111.0" layoutY="175.0" mnemonicParsing="false" text="Cancel" />
   </children>
</AnchorPane>
